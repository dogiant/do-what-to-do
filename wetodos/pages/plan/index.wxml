        <view class="weui-panel weui-panel_access">
            <view class="weui-panel__hd">学习计划列表</view>
            <view class="weui-panel__bd">
                <block wx:for="{{learningPlans}}" wx:for-index="idx" wx:for-item="plan">
                  <navigator url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
                      <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                          <image class="weui-media-box__thumb" src="{{plan.coverPicUrl}}" />
                      </view>
                      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                          <view class="weui-media-box__title">{{plan.name}}</view>
                          <view class="weui-media-box__desc">{{plan.digest}}</view>
                          <view class="weui-media-box__info">
                            <view class="weui-media-box__info__meta">文字来源</view>
                            <view class="weui-media-box__info__meta">时间</view>
                            <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">其它信息</view>
                          </view>
                      </view>
                  </navigator>
                </block>

