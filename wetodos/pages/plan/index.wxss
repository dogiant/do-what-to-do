.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}

.title-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: white;
}
.title {
  font-size: 18px;
  font-weight: bold;
  padding-top: 12px;
  padding-left: 12px;
}
.more {
  font-size: 12px;
  padding-top: 16px;
  padding-right: 12px;
  color: grey;
}
.plan-panel{
  background-color:#fff;
  margin-top:10px;
}
navigator {
  height:auto;
  width:auto;
  display:block;
  background-color:#fff;
}
.plan-media-box{
  display: flex;
  flex-direction: row;
  padding: 15px;
  position: relative;
  font-size: 13px;
}
.plan-image-box {
  margin-right: .8em;
  width: 130px;
  height: 100px;
  text-align:center;
}
.plan-media-box_active{
  background-color: ghostwhite;
}
.plan-cover {
  width: 100%;
  height: 100%;
  overflow: hidden;
  vertical-align:top;
}
.title-line{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.name {
  font-size: 14px;
  font-weight: bold;
}
.read-info{
  display: flex;
  flex-direction: row;
  margin-top:15px;
  padding-bottom:5px;
  font-size:13px;
  color:grey;
  line-height:1em;
  overflow:hidden;
}
.meta-extra{
  border-left:1px solid #cecece;
  margin-left: 10px;
  padding-left: 10px;
}