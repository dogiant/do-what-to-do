				<mvn.basicdb.jdbc.user>admin</mvn.basicdb.jdbc.user>
				<mvn.basicdb.jdbc.password>bk_test_*()</mvn.basicdb.jdbc.password>
				<mvn.basicdb.jdbc.url>jdbc:mysql://192.168.166.17:3306,192.168.166.17:3306/demo?useUnicode=true&amp;characterEncoding=UTF-8</mvn.basicdb.jdbc.url>
				
            </properties>
        </profile>
        <!--test env properties-->
        <profile>
            <id>test</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <properties>
            	<!-- 数据库连接 -->
				<mvn.basicdb.jdbc.user>admin</mvn.basicdb.jdbc.user>
				<mvn.basicdb.jdbc.password>bk_test_*()</mvn.basicdb.jdbc.password>
				<mvn.basicdb.jdbc.url>jdbc:mysql://192.168.166.17:3306,192.168.166.17:3306/dcvendor?useUnicode=true&amp;characterEncoding=UTF-8</mvn.basicdb.jdbc.url>
				
